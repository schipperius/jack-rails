<h1>Listing images</h1>

<table>
  <tr>
    <th>Image</th>
    <th>Title</th>
    <th>Lat</th>
    <th>Lng</th>
    <th>Title</th>
    <th>Caption</th>
    <th>Cite</th>
    <th>Footnote</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @images.each do |image| %>
  <tr>
    <td><%= image.image_id %></td>
    <td><%= image.title %></td>
    <td><%= image.lat %></td>
    <td><%= image.lng %></td>
    <td><%= image.title %></td>
    <td><%= image.caption %></td>
    <td><%= image.cite %></td>
    <td><%= image.footnote %></td>
    <td><%= link_to 'Show', image %></td>
    <td><%= link_to 'Edit', edit_image_path(image) %></td>
    <td><%= link_to 'Destroy', image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Image', new_image_path %>
